   "source": [
    "# Original string\n",
    "s = \"helloworld\"\n",
    "\n",
    "# Find the midpoint\n",
    "n = len(s) // 2\n",
    "\n",
    "# Split and change case\n",
    "first = s[:n].lower()    # first half to lowercase\n",
    "last = s[n:].upper()     # second half to uppercase\n",
    "\n",
    "# Concatenate\n",
    "result = first + last\n",
    "\n",
    "# Print the result\n",
    "print(result)\n"
   ]
  }
 ],
