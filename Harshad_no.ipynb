   "source": [
    "n = 163\n",
    "temp = n\n",
    "sum_digits = 0\n",
    "\n",
    "# Calculate sum of digits\n",
    "while n > 0:\n",
    "    digit = n % 10\n",
    "    sum_digits += digit\n",
    "    n //= 10\n",
    "\n",
    "# Check Harshad condition\n",
    "if temp % sum_digits == 0:\n",
    "    print(f\"{temp} is a Harshad Number\")\n",
    "else:\n",
    "    print(f\"{temp} is Not a Harshad Number\")\n"
   ]
  }
